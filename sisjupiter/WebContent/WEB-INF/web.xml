<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>br.com.sisjupiter.bo.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeBO</servlet-name>
        <servlet-class>br.com.sisjupiter.bo.HomeBO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServicoBO</servlet-name>
        <servlet-class>br.com.sisjupiter.bo.ServicoBO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AndEquipeBO</servlet-name>
        <servlet-class>br.com.andsisjupiter.bo.AndEquipeBO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AndComunidadeBO</servlet-name>
        <servlet-class>br.com.andsisjupiter.bo.AndComunidadeBO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GraficoBO</servlet-name>
        <servlet-class>br.com.sisjupiter.bo.GraficoBO</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeBO</servlet-name>
        <url-pattern>/HomeBO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServicoBO</servlet-name>
        <url-pattern>/ServicoBO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AndEquipeBO</servlet-name>
        <url-pattern>/andequipebo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AndComunidadeBO</servlet-name>
        <url-pattern>/andcomunidadebo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GraficoBO</servlet-name>
        <url-pattern>/GraficoBO</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
